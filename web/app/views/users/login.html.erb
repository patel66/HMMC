<div style="text-align:center">
  <h3>Login</h3>

  <%= form_tag("/users", method: "post") do %>
  <%= label_tag(:email, "Email") %>
  <%= text_field(:user, :email) %>
  <%= label_tag(:password, "Password") %>
  <%= text_field(:user, :password) %>
  <% label_tag(:School_Information)%>
  <br>
  <%= submit_tag("ENTER") %>
  <%end%>



</div>